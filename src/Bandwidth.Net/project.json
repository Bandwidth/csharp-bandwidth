{
  "version": "3.0.2",
  "description": "Bandwidth.Net is client library for the Bandwidth Application Platform",
  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true
  },
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "tools": {
      "EWSoftware.SHFB": "2016.5.28",
      "EWSoftware.SHFB.NETFramework": "4.6.1"
  },
  "frameworks": {
    "netstandard1.4": {
      "dependencies": {
        "NETStandard.Library": "1.4.0",
        "System.Xml.XmlSerializer": "4.0.11"
      }
    },
    "net45": {
      "buildOptions": {
        "debugType": "full"
      },
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Serialization": "4.0.0.0"
      }
    }
  },
  "packOptions": {
      "projectUrl": "https://github.com/bandwidthcom/csharp-bandwidth",
      "iconUrl": "http://bandwidth.com/sites/all/themes/bandwidth/images/logo.png",
      "repository": {
          "type": "git",
          "url": "https://github.com/bandwidthcom/csharp-bandwidth"
      },
      "tags": [
          "bandwidth",
          "catapult"
      ]
  }
}
